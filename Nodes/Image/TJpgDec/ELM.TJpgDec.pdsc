  <releases>
    <release date="2023-03-08" version="0.9.0" tag="0.9.0" url="https://raw.githubusercontent.com/ARM-software/EndpointAI/tree/master/Nodes/Image/TJpgDec/cmsis-pack/ELM.TJpdDec.0.9.0.pack">
      - TJpgDec R0.03
    </release>
  </releases>

  <keywords>
    <!-- keywords for indexing -->
    <keyword>Cortex-M</keyword>
    <keyword>JPEG</keyword>
    <keyword>Decoder</keyword>
  </keywords>
  
